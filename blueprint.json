{
  "title": "Test Container",
  "description": "Could be anything really",
  "memory_usage": {
    "required": 96,
    "recommended": 96
  },
  "protocol": "https_and_http",
  "license": {
    "label": "GNU GENERAL PUBLIC LICENSE V3",
    "url": "http://www.gnu.org/copyleft/gpl.html"
  },
  "sudos": [
    "rm_test_entry.sh",
    "list_entries.sh"
  ],
  "packages": [
    {
      "descriptor": {
        "value": "http://files.phpmyadmin.net/phpMyAdmin/4.6.4/phpMyAdmin-4.6.4-all-languages.tar.gz",
        "extraction": "tar -xzpf"
      }
    },
    {
      "descriptor": {
        "value": "https://github.com/EnginesBlueprints/test_package.git"
      }
    },
    {
      "descriptor": {
        "value": "https://github.com/EnginesBlueprints/build_tests.git"
      }
    }
  ],
  "framework": {
    "identifier": "php",
    "custom_php_inis": [
      {
        "content": "#test_php_ini"
      }
    ],
    "web_server": {
      "identifier": "apache",
      "htaccess_files": [
        {
          "directory": "setup",
          "content": "#test htacess"
        }
      ],
      "httpd_configurations": [
        {
          "content": "#test_HTTP_CONF"
        }
      ]
    }
  },
  "repositories": [
    {
      "descriptor": {
        "value": "ppa:opencpn/opencpn"
      }
    }
  ],
  "os_packages": [
    {
      "name": "php-intl"
    },
    {
      "name": "php-zip"
    },
    {
      "name": "mysql-client"
    },
    {
      "name": "make"
    }
  ],
  "modules": [
    {
      "name": "django",
      "type": "python"
    },
    {
      "name": "node-red",
      "type": "npm"
    },
    {
      "name": "dkjson",
      "type": "lua"
    },
    {
      "name": "cairo",
      "type": "pecl"
    },
    {
      "name": "Math_Complex",
      "type": "pear"
    },
    {
      "name": "rstudio",
      "type": "r"
    },
    {
      "name": "intl",
      "type": "php"
    },
    {
      "name": "negotiation",
      "type": "apache"
    },
    {
      "name": "rewrite",
      "type": "apache"
    },
    {
      "name": "zip",
      "type": "php"
    },
    {
      "name": "headers",
      "type": "apache"
    }
  ],
  "bindings": [
    {
      "identifier": "file_system_1",
      "descriptor": {
        "value": "https://github.com/MarkRatjens/filesystem.git"
      }
    },
    {
      "identifier": "file_system_2",
      "descriptor": {
        "value": "https://github.com/MarkRatjens/filesystem.git"
      }
    },
    {
      "identifier": "db_1",
      "descriptor": {
        "value": "https://github.com/MarkRatjens/mysqld.git"
      },
      "variables": {
        "name": "name",
        "username": "username"
      }
    },
    {
      "identifier": "db_2",
      "descriptor": {
        "value": "https://github.com/MarkRatjens/mysqld.git"
      },
      "variables": {
        "name": "name",
        "username": "username"
      }
    }
  ],
  "environment": {
    "ports": [
      {
        "port": "12345",
        "external": "12345",
        "protocol": "tcp"
      },
      {
        "port": "12346",
        "external": "12346",
        "protocol": "udp"
      },
      {
        "port": "12347",
        "external": "12347",
        "protocol": "tcp_and_udp"
      }
    ],
    "variables": [
      {
        "name": "test_envvar",
        "value": "first",
        "ask_at_build_time": true,
        "input": {
          "type": "string"
        }
      },
      {
        "name": "mandatory",
        "mandatory": true,
        "ask_at_build_time": true,
        "input": {
          "type": "string",
          "title": "mandatory test"
        }
      },
      {
        "name": "buildtime_only",
        "ask_at_build_time": true,
        "build_time_only": true,
        "input": {
          "type": "string",
          "title": "buildtime_only test"
        }
      }
    ]
  },
  "permissions": [
    {
      "path": "test_package_dest/test_write_file"
    },
    {
      "path": "test_package_dest/test_write_dir"
    },
    {
      "path": "test_package_dest/test_write_rec_dir",
      "recursive": true
    }
  ],
  "replacements": [
    {
      "string": "/TBLE/s//TABLE/",
      "source": "/sql/create_tables.sql",
      "destination": "/sql/create_tables.sql"
    }
  ],
  "data": [
    {
      "path": "/home/app/fresh_dir_perm_test"
    },
    {
      "path": "/home/app/home_persistent"
    },
    {
      "path": "/usr/local/local_persist"
    },
    {
      "path": "/home/home_dir/home_dir_persist"
    },
    {
      "path": "/home/app/test_package_dest/test_persist_dir"
    },
    {
      "path": "/home/app/persistent",
      "name": "data"
    }
  ],
  "source": [
    {
      "path": "app/fresh_test_persistent_file"
    },
    {
      "path": "app/test_package_dest/test_persist_file",
      "name": "data"
    }
  ],
  "workers": {
    "blocking": "sleep 30000",
    "commands": [
      {
        "name": "test",
        "command": "ls -lR/home > /home/fs/test/home.ls-lR"
      }
    ]
  },
  "schedules": [
    {
      "name": "list_users",
      "action": "list_test",
      "time": {
        "minute": "13",
        "hour": "1",
        "day_of_month": "*",
        "month": "*",
        "day_of_week": "*"
      }
    }
  ],
  "actions": [
    {
      "name": "list_test",
      "return_type": "plain_text"
    },
    {
      "name": "add_nvp",
      "label": "Add entry",
      "return_type": "plain_text",
      "variables": [
        {
          "name": "name",
          "input": {
            "type": "string"
          }
        },
        {
          "name": "value",
          "input": {
            "type": "string"
          }
        }
      ]
    },
    {
      "name": "rm_test_entry",
      "label": "Remove test entry",
      "return_type": "none",
      "variables": [
        {
          "name": "name",
          "input": {
            "type": "string"
          }
        }
      ]
    },
    {
      "name": "ls_persist",
      "return_type": "plain_text"
    }
  ]
}
